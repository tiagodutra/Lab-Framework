<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<navigation-rule>
		<navigation-case>
			<from-outcome>NavegacaoExplicitaIndex</from-outcome>
			<to-view-id>/lab4/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>NavegacaoExplicitaPagina1</from-outcome>
			<to-view-id>/lab4/pagina1.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<application>
		<locale-config>
			<default-locale>pt_BR</default-locale>
			<supported-locale>pt_BR</supported-locale>
			<supported-locale>en_US</supported-locale>
		</locale-config>
		<resource-bundle>
			<base-name>threeway.projeto.lab5.messages</base-name>
			<var>msg</var>
		</resource-bundle>
		<message-bundle>threeway.projeto.lab5.messages
</message-bundle>
</application>

</faces-config>
